<?xml version="1.0" encoding="utf-8" ?>
<web-app
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">

	<display-name>mystore</display-name>

	<listener>
		<listener-class>com.genexus.webpanels.ServletEventListener</listener-class>
	</listener>
	<listener>
		<listener-class>com.genexus.webpanels.SessionEventListener</listener-class>
	</listener>
	<servlet>
		<servlet-name>com.mystore.rwdmasterpage</servlet-name>
		<servlet-class>com.mystore.rwdmasterpage</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>com.mystore.rwdpromptmasterpage</servlet-name>
		<servlet-class>com.mystore.rwdpromptmasterpage</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>com.mystore.rwdrecentlinks</servlet-name>
		<servlet-class>com.mystore.rwdrecentlinks</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>com.mystore.gx0010</servlet-name>
		<servlet-class>com.mystore.gx0010</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>com.mystore.wphome</servlet-name>
		<servlet-class>com.mystore.wphome</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>com.mystore.cliente</servlet-name>
		<servlet-class>com.mystore.cliente</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>com.mystore.rwdmasterpage</servlet-name>
		<url-pattern>/servlet/com.mystore.rwdmasterpage</url-pattern>
		<url-pattern>/com.mystore.rwdmasterpage</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>com.mystore.rwdpromptmasterpage</servlet-name>
		<url-pattern>/servlet/com.mystore.rwdpromptmasterpage</url-pattern>
		<url-pattern>/com.mystore.rwdpromptmasterpage</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>com.mystore.rwdrecentlinks</servlet-name>
		<url-pattern>/servlet/com.mystore.rwdrecentlinks</url-pattern>
		<url-pattern>/com.mystore.rwdrecentlinks</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>com.mystore.gx0010</servlet-name>
		<url-pattern>/servlet/com.mystore.gx0010</url-pattern>
		<url-pattern>/com.mystore.gx0010</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>com.mystore.wphome</servlet-name>
		<url-pattern>/servlet/com.mystore.wphome</url-pattern>
		<url-pattern>/com.mystore.wphome</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>com.mystore.cliente</servlet-name>
		<url-pattern>/servlet/com.mystore.cliente</url-pattern>
		<url-pattern>/com.mystore.cliente</url-pattern>
	</servlet-mapping>

	<context-param>
		<param-name>gxcfg</param-name>
		<param-value>com.mystore.GXcfg</param-value>
	</context-param>
	<filter>
		<filter-name>ExpiresFilter</filter-name>
		<filter-class>com.genexus.filters.ExpiresFilter</filter-class>
		<init-param>
			<param-name>ExpiresByType image</param-name>
			<param-value>access plus 36 hours</param-value>
		</init-param>
		<init-param>
			<param-name>ExpiresByType text/css</param-name>
			<param-value>access plus 36 hours</param-value>
		</init-param>
		<init-param>
			<param-name>ExpiresByType application/javascript</param-name>
			<param-value>access plus 36 hours</param-value>
		</init-param>
	</filter>

	<filter-mapping>
		<filter-name>ExpiresFilter</filter-name>
		<url-pattern>/*</url-pattern>
		<dispatcher>REQUEST</dispatcher>
	</filter-mapping>

	<filter>
		<filter-name>InternalRestServicesFilter</filter-name>
		<filter-class>com.genexus.filters.InternalRestServicesFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>InternalRestServicesFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>	


</web-app> 